<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ImageView
            android:layout_width="match_parent"
            android:layout_height="320dp"

            app:srcCompat="@mipmap/ic_launcher" android:id="@+id/imageView4" tools:ignore="ContentDescription"/>
    <RelativeLayout android:layout_width="wrap_content"
                  android:layout_height="wrap_content">
        <TextView

                android:hint="TextView"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_height="33dp" tools:layout_editor_absoluteY="296dp"
                android:id="@+id/textView3" tools:ignore="MissingConstraints" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.619"
                android:layout_alignParentStart="true" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_marginStart="0dp" android:visibility="visible"/>
        <ImageView
                android:layout_width="33dp"

                android:layout_height="33dp" app:srcCompat="@drawable/ic_border_color_black"
                tools:layout_editor_absoluteY="296dp" android:id="@+id/imageView"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="MissingConstraints" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_alignParentEnd="true"/>
</RelativeLayout>

    <RelativeLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
        <TextView
                android:hint="TextView"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_height="33dp" tools:layout_editor_absoluteY="296dp"
                android:id="@+id/textView4" tools:ignore="MissingConstraints" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.619"
                android:layout_alignParentStart="true" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_marginStart="0dp"/>
        <ImageView
                android:layout_width="33dp"

                android:layout_height="33dp" app:srcCompat="@drawable/ic_border_color_black"
                tools:layout_editor_absoluteY="296dp" android:id="@+id/imageView1"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="MissingConstraints" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>


    <RelativeLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
        <TextView
                android:hint="TextView"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_height="33dp" tools:layout_editor_absoluteY="296dp"
                android:id="@+id/textView5" tools:ignore="MissingConstraints" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.619"
                android:layout_alignParentStart="true" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_marginStart="0dp"/>
        <ImageView
                android:layout_width="33dp"

                android:layout_height="33dp" app:srcCompat="@drawable/ic_border_color_black"
                tools:layout_editor_absoluteY="296dp" android:id="@+id/imageView2"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="MissingConstraints" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>


    <RelativeLayout android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
        <TextView
                android:hint="TextView"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_height="33dp" tools:layout_editor_absoluteY="296dp"
                android:id="@+id/textView6" tools:ignore="MissingConstraints" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.619"
                android:layout_alignParentStart="true" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_marginStart="0dp"/>
        <ImageView
                android:layout_width="33dp"

                android:layout_height="33dp" app:srcCompat="@drawable/ic_border_color_black"
                tools:layout_editor_absoluteY="296dp" android:id="@+id/imageView3"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="ContentDescription,MissingConstraints" android:layout_alignParentTop="true"
                android:layout_marginTop="0dp" android:layout_alignParentEnd="true"/>
    </RelativeLayout>

    <LinearLayout android:layout_width="wrap_content"
                  android:orientation="horizontal"
                  android:layout_height="wrap_content">

        <Button android:layout_width="205dp"
                android:text="Calendario"
                android:layout_height="wrap_content" tools:ignore="ButtonStyle,HardcodedText"/>
        <Button android:layout_width="205dp"
                android:text="Elimina"

                android:layout_height="wrap_content" tools:ignore="ButtonStyle,HardcodedText"/>

    </LinearLayout>
</LinearLayout>